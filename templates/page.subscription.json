    "tabs_faq_idqDkf": {
      "type": "tabs-faq",
      "blocks": {
        "text_cX4rbb": {
          "type": "text",
          "settings": {
            "title": "WHAT ARE THE BENEFITS OF HAVING A SUBSCRIPTION?",
            "description": "<p>Wellnesse subscription customers gain access to exclusive discounts and automated delivery schedules customized to fit their specific needs. You’ll never have to worry about running out, re-ordering, or paying full price!<\/p>",
            "img_orientation": "landscape",
            "img_crop": "center",
            "img_thumbnail": "",
            "img_align_desktop": "flex-lg-row",
            "external_video": "",
            "force_lazyload": false
          }
        },
        "text_KNNzj6": {
          "type": "text",
          "settings": {
            "title": "CAN I CHANGE MY SUBSCRIPTION?",
            "description": "<p>Absolutely! You can update your order frequency and your subscription items at any time by logging into your customer account.<\/p>",
            "img_orientation": "landscape",
            "img_crop": "center",
            "img_thumbnail": "img-thumbnail",
            "img_align_desktop": "flex-lg-row",
            "external_video": "",
            "force_lazyload": true
          }
        },
        "text_36NMAJ": {
          "type": "text",
          "settings": {
            "title": "WHERE DO YOU SHIP?",
            "description": "<p>We ship within the United States, PO Boxes, APO, Australia, Canada, Denmark, Finland, France, Germany, Ireland, Italy, Mexico, Netherlands, New Zealand, Norway, Portugal, South Africa, Sweden, Switzerland, and the United Kingdom.<\/p>",
            "img_orientation": "landscape",
            "img_crop": "center",
            "img_thumbnail": "img-thumbnail",
            "img_align_desktop": "flex-lg-row",
            "external_video": "",
            "force_lazyload": true
          }
        },
        "text_9rb8P3": {
          "type": "text",
          "settings": {
            "title": "ARE WELLNESSE PRODUCTS ALL NATURAL?",
            "description": "<p>Yes! We formulate our personal care products with safe, all natural ingredients. Wellnesse products are always cruelty-free, GMO-free, sulfate-free, paraben-free, and phthalate-free.<\/p>",
            "img_orientation": "landscape",
            "img_crop": "center",
            "img_thumbnail": "img-thumbnail",
            "img_align_desktop": "flex-lg-row",
            "external_video": "",
            "force_lazyload": true
          }
        },
        "text_86DApr": {
          "type": "text",
          "settings": {
            "title": "Can I order as a gift for someone else?",
            "description": "<p>When ordering as a gift for someone, be sure to enter in your email address to receive all summary information but enter in the recipient’s name and shipping information. We will be sure to keep all prices and receipts out of the package.<\/p>",
            "img_orientation": "landscape",
            "img_crop": "center",
            "img_thumbnail": "img-thumbnail",
            "img_align_desktop": "flex-lg-row",
            "external_video": "",
            "force_lazyload": true
          }
        }
      },

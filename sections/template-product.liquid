{% liquid
    assign pt = section.settings.pt | prepend: 'pt-5 pt-sm-'
    assign pb = section.settings.pb | prepend: 'pb-'

    assign variants_json = product.variants | json
%}

<a class="visually-hidden-focusable" href="#product-content">
    {{ 'general.accessibility.skip_product_content' | t }}
</a>

<section 
    id="product-template"
    data-p-id="6253269352530:120"
    class="{{ pt }} {{ pb }}"
    data-product-handle="{{ product.handle }}">
    <div class="container">
        {% if section.settings.show_breadcrumbs %}
            <div class="breadcrumbs-wrapper mb-6">
                {% render 'breadcrumbs' %}
            </div>
        {% endif %}
        <div class="row">
            <div class="col-lg-6">
                <div class="product-gallery-wrapper">
                    {% render 'product-gallery' %}
                    {% render 'product-block-testimonial', suffix: '-gallery' %}
                </div>
            </div>
            <div class="col-lg-6">
                <div id="product-content" class="product-content">
                    {% form 'product', product, onsubmit: 'onSubmitAtcForm(this, event)', data-variants: variants_json %}
                        <input type="hidden" name="id" value="{{ product.selected_or_first_available_variant.id }}">
                        {% liquid
                            for block in section.blocks
                                case block.type
                                    when 'title'
                                        render 'product-block-title', block: block
                                    when 'description'
                                        render 'product-block-description', block: block
                                    when 'price'
                                        render 'product-block-price', block: block, form: form
                                    when 'variant_selector'
                                        render 'product-block-variant-selector', block: block
                                    when 'purchase_options'
                                        render 'product-block-purchase-options', block: block
                                    when 'buy_buttons'
                                        render 'product-block-buy-buttons', block: block, form: form
                                    when 'personalization'
                                        render 'product-block-personalization', block: block
                                    when 'accordion'
                                        render 'product-block-accordion', block: block
                                    when 'inventory_bar'
                                        render 'product-block-inventory-bar', block: block
                                    when 'qty_breaks'
                                        render 'product-block-qty-breaks', block: block
                                    when 'delivery_info'
                                        render 'product-block-delivery-info', block: block
                                    when 'trust_badges'
                                        render 'product-block-trust-badges', block: block
                                    when 'text'
                                        render 'product-block-text', block: block
                                    when 'liquid_code'
                                        render 'product-block-liquid-code', block: block
                                    when 'limited_offer'
                                        render 'product-block-limited-offer', block: block
                                    when 'store_availability'
                                        render 'product-block-store-availability', block: block
                                    when 'separator'
                                        render 'product-block-separator', block: block
                                    when 'media'
                                        render 'product-block-media', block: block
                                    when 'testimonial'
                                        render 'product-block-testimonial', block: block
                                    when 'variant_description'
                                        render 'product-block-variant-description', block: block
                                    when 'brand_image'
                                        render 'product-block-brand-image', block: block
                                    when 'product_switcher'
                                        render 'product-block-product-switcher', block: block
                                   when 'free_gift_notice'
                                        render 'product-block-freegift-notice', block: block
                                    when 'marquee'
                                        render 'product-block-marquee', block: block
                                    when 'stories'
                                        render 'product-block-stories', block: block
                                    when 'page-content'
                                        render 'product-block-page-content', block: block
                                    when 'collection_link'
                                        render 'product-block-collection', block: block
                                    when '@app'
                                        render block
                                endcase
                            endfor
                        %}

                    {% endform %}

                    {% liquid
                        render 'product-bought-together' 
                        render 'product-contact-form'
                    %}

                    {% if section.settings.show_share_btn %}
                        <div class="d-flex justify-content-end">
                            {% render 'share-button', title: product.title, url: product.url %}
                        </div>
                    {% endif %}
                </div>
            </div>
        </div>
    </div>
</section>

{% liquid
    render 'product-block-upsell-modal'
    render 'product-block-size-chart'
    render 'product-prev-next'
%}

{%- liquid
    if product.selected_or_first_available_variant.featured_media
        assign seo_media = product.selected_or_first_available_variant.featured_media
    else
        assign seo_media = product.featured_media
    endif
-%}

<script type="application/ld+json">
{
    "@context": "http://schema.org/",
    "@type": "Product",
    "name": {{ product.title | json }},
    "url": {{ request.origin | append: product.url | json }},
    {% if seo_media -%}
        "image": [
            {{ seo_media | image_url: width: seo_media.preview_image.width | prepend: "https:" | json }}
        ],
    {%- endif %}
    "description": {{ product.description | strip_html | json }},
    {% if product.selected_or_first_available_variant.sku != blank -%}
        "sku": {{ product.selected_or_first_available_variant.sku | json }},
    {%- endif %}
    "brand": {
        "@type": "Brand",
        "name": {{ product.vendor | json }}
    },
    "offers": [
        {%- for variant in product.variants -%}
            {
            "@type" : "Offer",
            {%- if variant.sku != blank -%}
                "sku": {{ variant.sku | json }},
            {%- endif -%}
            {%- if variant.barcode.size == 12 -%}
                "gtin12": {{ variant.barcode }},
            {%- endif -%}
            {%- if variant.barcode.size == 13 -%}
                "gtin13": {{ variant.barcode }},
            {%- endif -%}
            {%- if variant.barcode.size == 14 -%}
                "gtin14": {{ variant.barcode }},
            {%- endif -%}
            "availability" : "http://schema.org/{% if variant.available %}InStock{% else %}OutOfStock{% endif %}",
            "price" : {{ variant.price | divided_by: 100.00 | json }},
            "priceCurrency" : {{ cart.currency.iso_code | json }},
            "url" : {{ request.origin | append: variant.url | json }}
            }{% unless forloop.last %},{% endunless %}
        {%- endfor -%}
    ]
}
</script>

